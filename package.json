{
  "name": "thaumaturge",
  "version": "0.7.0",
  "repository": "git@github.com:maxdeviant/thaumaturge.git",
  "author": "Marshall Bowers <elliott.codes@gmail.com>",
  "license": "MIT",
  "description": "Thaumaturge is a fixtures and seeding library powered by `io-ts`.",
  "keywords": [
    "thaumaturge",
    "typescript",
    "io-ts",
    "test",
    "testing",
    "fixture",
    "factory",
    "seed",
    "seeding"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --write '**/*.+(js|json|md|ts|tsx)'",
    "build": "tsc -p .",
    "test": "jest"
  },
  "files": [
    "lib/",
    "!lib/**/__tests__/",
    "!lib/**/*.test.{js,d.ts}"
  ],
  "peerDependencies": {
    "fp-ts": "2.x",
    "io-ts": "2.x"
  },
  "dependencies": {
    "@faker-js/faker": "6.0.0-alpha.7",
    "type-fest": "2.12.2"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "fp-ts": "2.11.8",
    "io-ts": "2.2.16",
    "io-ts-types": "0.5.16",
    "jest": "27.5.1",
    "monocle-ts": "2.3.12",
    "newtype-ts": "0.3.5",
    "prettier": "2.5.1",
    "sqlite": "4.0.23",
    "sqlite3": "5.0.2",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  }
}
